{
  "name": "app-sst",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run sst",
    "build": "next build",
    "devnext": "next dev --port 3001 --turbopack",
    "lint": "next lint",
    "start": "next start",
    "unlock": "npx sst unlock --stage development",
    "sst": "npm run unlock; npm run sst:load:dev; npx sst dev --stage development",
    "sst:load:all": "npm run sst:load:dev; npm run sst:load:preview; npm run sst:load:production;",
    "sst:load:dev": "npx sst secret load .env.local --stage development",
    "sst:load:preview": "npx sst secret load .env.local --stage preview",
    "sst:load:production": "npx sst secret load .env.local --stage production",
    "sst:deploy:preview": "npm run sst:load:preview; npx sst deploy --stage preview",
    "sst:deploy:production": "npm run sst:load:production; npx sst deploy --stage production",
    "sst:remove:prod:forever": "",
    "sst:remove:preview:forever": "npx sst remove --stage preview",
    "sst:remove:dev": "npx sst remove --stage development"
  },
  "dependencies": {
